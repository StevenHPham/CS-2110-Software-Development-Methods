
public class inclass5 {

	public static void main(String[] args) {
		double x = 1.0;
		double y = 4.2;
		double res = points(x,y);
		System.out.println(res);
		
		double a = 4.2;
		double b = 1.0;
		double res = points(a,b);
		System.out.println(res);
		
		double c = -1.0;
		double d = -4.2;
		double res = points(c,d);
		System.out.println(res);
		
		double e = -1.0;
		double f = 4.2;
		double res = points(e,f);
		System.out.println(res);
		
	
	}

	public static double points (double x, double y) {
		double x = 4;
		double y = 0.5;
		if (x > 0 && y > 0) {
			return x*y;
		}
			
		if (x < 0 && y >= 0) {
			return x-y;
		}
		if ( x < 0 && y < 0) {
			return x / y;
		}
		else {
			return x + y;
		}
			
					
	}
}
